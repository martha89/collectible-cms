{
  "name": "collectible-cms",
  "version": "0.0.1",
  "description": "API backend for the Collectible Content Management System",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "apidoc -i server/ -o apidoc/v1/ && apidoc -i server/ -o apidoc/v1/ -t templates/apidoc",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "start": "concurrent \"npm run client\" \"npm run server\" "
  },
  "author": "Bryan Hazelbaker",
  "license": "MIT",
  "dependencies": {
    "apidoc": "^0.16.1",
    "body-parser": "~1.15.2",
    "crypto": "0.0.3",
    "express": "~4.14.0",
    "fs": "0.0.2",
    "jsonwebtoken": "~7.1.7",
    "mongoose": "~4.5.8",
    "morgan": "~1.7.0"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "nodemon": "^1.10.2"
  }
}
